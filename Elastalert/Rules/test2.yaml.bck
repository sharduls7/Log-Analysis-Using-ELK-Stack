name: Test Rule

type: frequency

index: cafelog9

num_events: 1

timeframe:
  minutes: 5

aggregation:
  minutes: 2

filter:
- term:
    response: "403"

alert:
- "email"

from_addr: "noreply@elasticsearch.org"

alert_subject: "ELK Alert-'{0}': PFB table of IPs with malicious request attempts"

alert_subject_args:
- name

alert_text_type: aggregation_summary_only

summary_table_fields:
    - clientip
    - num_hits

email:
- "shardul.sant@amdocs.com"

cc: 
- "shardul.sant@amdocs.com"

smtp_host: "punemail.corp.amdocs.com"
#smtp_host: stlmail.corp.amdocs.com
smtp_port: "25"
smtp_ssl: false